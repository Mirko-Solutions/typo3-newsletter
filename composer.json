{
  "name": "mirko/typo3-newsletter",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension to send any pages as Newsletter and provide statistics on opened emails and clicked links.",
  "homepage": "https://github.com/Mirko-Solutions/typo3-newsletter",
  "license": "GPL-2.0+",
  "version": "1.0.0",
  "keywords": [
    "TYPO3 CMS",
    "TYPO3",
    "newsletter",
    "email",
    "statistics"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-dom": "*",
    "ext-openssl": "*",
    "typo3/cms-backend": ">=9.5",
    "typo3/cms-core": ">=9.5",
    "typo3/cms-extbase": ">=9.5",
    "typo3/cms-fluid": ">=9.5",
    "typo3/cms-scheduler": ">=9.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Mirko\\Typo3Newsletter\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mirko\\Typo3Newsletter\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    }
  }
}
